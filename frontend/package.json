{
  "name": "xray",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5175",
    "dev:silent": "cross-env SASS_SILENCE_DEPRECATIONS=true npm run dev",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File dev-silent.ps1 -Dev",
    "build": "vite build",
    "build:production": "cross-env NODE_ENV=production SUPPRESS_SASS_WARNINGS=true vite build",
    "build:deployment": "node deployment-build.js",
    "build:clean": "node deployment-build.js",
    "build:silent": "powershell -ExecutionPolicy Bypass -File dev-silent.ps1 -Build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:silent": "powershell -ExecutionPolicy Bypass -File dev-silent.ps1 -Preview",
    "check:warnings": "npm run build 2>&1 | grep -i 'warning\\|deprecation' || echo 'No warnings found!'",
    "deployment:ready": "npm run build:deployment && echo 'Deployment package ready!'"
  },
